{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@/components/uni-$1/uni-$1.vue",
      "^xxt-(.*)": "@/components/xxt-components/xxt-$1/xxt-$1.vue",
      "^tui-(.*)": "@/components/thorui/tui-$1/tui-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "name": "home",
      "style": {
        "navigationBarTitleText": "首页uni-app",
        "navigationBarBackgroundColor": "#FFFFFF",
        "navigationBarTextStyle": "black"
      }
    },
    {
      "path": "pages/detail/index",
      "name": "detail",
      "style": {
        "navigationBarTitleText": "我是功能列表",
        "navigationBarBackgroundColor": "#27C675",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/detail/test",
      "name": "test",
      "style": {
        "navigationBarTitleText": "我是test",
        "navigationBarBackgroundColor": "#27C675",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/login/index",
      "name": "login",
      "style": {
        "navigationBarTitleText": "我是login",
        "navigationBarBackgroundColor": "#27C675",
        "navigationBarTextStyle": "white"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/images/tabbar/tabbar_home.png",
        "selectedIconPath": "static/images/tabbar/tabbar_home1.png"
      },
      {
        "pagePath": "pages/detail/index",
        "text": "功能",
        "iconPath": "static/images/tabbar/tabbar_category.png",
        "selectedIconPath": "static/images/tabbar/tabbar_category1.png"
      }
    ]
  }
}